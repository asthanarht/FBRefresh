<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/fb.css")
</head>
<body>
    <div id="container">
        <div id="header">
            <h1 id="logo">
                <a href="#"><img src="http://www.erhanbasa.com/uploads/old/fb/facebook-logo.png" /></a>
            </h1>
            <ul id="jewel-container">
                <li class="jewel requests">
                    <a href="#"></a>
                </li>
                <li class="jewel messages">
                    <a href="#"></a>
                </li>
                <li class="jewel notifications">
                    <a href="#"></a>
                </li>
            </ul>
            <ul id="head-navigation">
                <li><a href="#" class="current">News</a></li>
                <li><a href="#">Messages</a></li>
                <li><a href="#">Events</a></li>
                <li><a href="#">Photos</a></li>
                <li><a href="#">Find Friends</a></li>
            </ul>
            <div id="head-search">
                <span>Find Friends</span>
                <form id="searchform">
                    <input placeholder="Search">
                    <input type="button" class="btn">
                </form>
            </div>
        </div>
    </div>
 
            @RenderBody()
       
    
</body>
</html>
